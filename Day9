#include<iostream>
#include<list>
using namespace std;

class YouTubeChannel {
public:
	string Name;
	string OwnerName;
	int SubscribersCount;
	list<string> PublishedVideoTitles;
};

int main()

{
	YouTubeChannel ytChannel;
	ytChannel.Name = "TusherTheSnow";
	ytChannel.OwnerName = "tusher";
	ytChannel.SubscribersCount = 1800;
	ytChannel.PublishedVideoTitles = { "c++ for beginners video 1", "HTML", "c++ OOP" };

	cout << "Name: " << ytChannel.Name << endl;
	cout << "OwnerName: " << ytChannel.OwnerName << endl;
	cout << "Subscribtion: " << ytChannel.SubscribersCount << endl;

	cout << "Videos: " << endl;
	for (string videoTitle : ytChannel.PublishedVideoTitles) {
		cout << videoTitle << endl;
	}


	system("pause>0");
}
